#include <stdio.h>
    int main()
    {
        int a,b,c;
        
        printf("Enter a = \n");
        scanf("%d", &a);// for value of a 
        
        printf("Enter b = \n");
        scanf("%d", &b);// for value of b

         printf("Enter c = \n");
         scanf("%d",&c);//for value of c

        if(a>b && a>c){
            printf("a is grester than b and c\n");
        }
        else if(b>a && b>c){
            printf("b is grester than a and c\n");
        }
        else if(c>b && c>a){
            printf("c is greater than b and a\n");
        }

        return 0;
    }
